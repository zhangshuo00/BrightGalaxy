/* pages/me/me.wxss */

.headbox{
  background: rgb(50, 179, 140);
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.headimg{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
}
.username{
  font-size: 35rpx;
  margin-top: 20rpx;
}
.outlogin{
  position: absolute;
  width: 100%;
  bottom: 100rpx;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height:inherit;
  border: 0;
}
button {
  border-radius:0;
}
.button-hover{
  background-color: transparent;
}
